{
  "name": "Task manager",
  "version": "1.0.0",
  "description": "Manages tasks",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "node backend/app.js",
    "start-frontend": "npm start --prefix frontend",
    "test": "mocha 'backend/test/**/*.js' --exit",
    "test:watch": "mocha --watch 'backend/tests/**/*.js'"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.2.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mocha": "^10.7.0",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0"
  }
}
